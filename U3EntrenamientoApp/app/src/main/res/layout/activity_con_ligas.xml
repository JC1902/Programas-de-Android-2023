<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SinInstrumentosActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#AAEAEC"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="@string/app_name"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar2"
        app:layout_constraintEnd_toEndOf="@+id/toolbar2"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintStart_toStartOf="@+id/toolbar2"
        app:layout_constraintTop_toTopOf="@+id/toolbar2"
        app:layout_constraintVertical_bias="0.432" />

    <ImageButton
        android:id="@+id/ibtnLiga1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:onClick="btnEjercicioUno"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2"
        app:srcCompat="@drawable/liga1" />

    <ImageButton
        android:id="@+id/ibtnLiga2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="52dp"
        android:onClick="btnEjercicioUno"
        app:layout_constraintStart_toEndOf="@+id/ibtnLiga1"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2"
        app:srcCompat="@drawable/liga2" />

    <ImageButton
        android:id="@+id/ibtnLiga3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginTop="40dp"
        android:onClick="btnEjercicioUno"
        app:layout_constraintStart_toEndOf="@+id/ibtnLiga2"
        app:layout_constraintTop_toBottomOf="@+id/toolbar2"
        app:srcCompat="@drawable/liga3" />

    <ImageButton
        android:id="@+id/ibtnLiga4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:onClick="btnEjercicioUno"
        app:layout_constraintTop_toBottomOf="@+id/ibtnLiga2"
        app:srcCompat="@drawable/liga4"
        tools:layout_editor_absoluteX="11dp" />

    <ImageButton
        android:id="@+id/ibtnLiga7"
        android:layout_width="161dp"
        android:layout_height="139dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="8dp"
        android:onClick="btnEjercicioUno"
        app:layout_constraintBottom_toTopOf="@+id/ibtnLiga9"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/liga5" />

    <ImageButton
        android:id="@+id/ibtnLiga5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="68dp"
        android:onClick="btnEjercicioUno"
        app:layout_constraintStart_toEndOf="@+id/ibtnLiga4"
        app:layout_constraintTop_toBottomOf="@+id/ibtnLiga2"
        app:srcCompat="@drawable/liga6" />

    <ImageButton
        android:id="@+id/ibtnLiga6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="60dp"
        android:onClick="btnEjercicioUno"
        app:layout_constraintStart_toEndOf="@+id/ibtnLiga5"
        app:layout_constraintTop_toBottomOf="@+id/ibtnLiga2"
        app:srcCompat="@drawable/liga7" />

    <ImageButton
        android:id="@+id/ibtnLiga9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:onClick="btnEjercicioUno"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/liga8" />

    <ImageButton
        android:id="@+id/ibtnLiga8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:onClick="btnEjercicioUno"
        app:layout_constraintBottom_toTopOf="@+id/ibtnLiga9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.64"
        app:layout_constraintStart_toEndOf="@+id/ibtnLiga7"
        app:srcCompat="@drawable/liga9" />

    <ImageButton
        android:id="@+id/imbtnRegresar"
        android:layout_width="56dp"
        android:layout_height="49dp"
        android:background="#00FFFFFF"
        android:onClick="imbtnRegresarClick"
        app:layout_constraintBottom_toBottomOf="@+id/toolbar2"
        app:layout_constraintEnd_toEndOf="@+id/toolbar2"
        app:layout_constraintHorizontal_bias="0.895"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.466"
        app:srcCompat="@android:drawable/ic_delete" />

</androidx.constraintlayout.widget.ConstraintLayout>